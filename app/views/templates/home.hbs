<div class="col-md-1 col-sm-2" id="three-w-when">
  <strong>When</strong>
  <p id="value">value</p>
  <input
    class="hidden-xs"
    type="range"
    min="1"
    max="31"
    step="1"
    value="1"
    data-orientation="vertical"
  >
  <br>
  <button id="play" class="btn">
    <i class="glyphicon glyphicon-play"></i>
  </button>
  <button id="pause" class="btn hide">
    <i class="glyphicon glyphicon-pause"></i>
  </button>
  {{!-- <input class="visible-xs" type="range"> --}}
</div>
<div class="col-md-11 col-sm-10">
  <div class="row">
    <div class="col-md-4 col-sm-6" id="three-w-who">
      <strong>Who (Top {{ top }})</strong><span class="reset" style="display: none;">: <span class="filter">
    </div>
    <div class="col-md-4 col-sm-6" id="three-w-what">
      <strong>What (Top {{ top }})</strong><span class="reset" style="display: none;">: <span class="filter">
    </div>
    <div class="col-md-4 col-sm-12">
      <strong>Where</strong>
      {{!-- <span id="three-w-count">: <span> --}}
      <div id="three-w-where"></div>
    </div>
  </div>
</div>
